n = int(input(""))
mulnum = 0
if n == 1:
    print(1)
while n > 1:
    nowlist = range(2,2**mulnum+1,2)
    for i in nowlist:
        n-=1
        if n < 2:
            print(i)
            break
    mulnum += 1